class Main {parantheses(s:String) : Bool {{  (let counter : Int <- 1, ans : Int <- 0, flag1 : Bool <- true in    {    while (not(counter-1 = s.length() ) ) loop      {      if s.substr(counter-1,1) = "(" then ans <- ans + 1        else ans <- ans - 1      fi;      if ans < 0 then {flag1 <- false; counter <- counter + 1;}      else counter <- counter + 1      fi;      }    pool;  if flag1 = false then false else    if ans = 0 then true else false fi  fi;  }  );  }  };   main() :Object {{	(new IO).out_string("\n");    if (parantheses((new IO).in_string()) = true) then (new IO).out_string("True") else	(new IO).out_string("False") fi;	(new IO).out_string("\n");	self;}    };};